{
  "version": 2,
  "rewrites": [
    {
      "source": "/ogimage/:filename",
      "destination": "/ogimage/:filename",
      "check": true
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": false,
  "headers": [
    {
      "source": "/ogimage/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "image/jpeg"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}